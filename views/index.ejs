              function checkLoginUsername() {
                  var user=$("#login-username").val();
                  if(validateLoginUser(user)){
                    $.ajax({
                          async: true,
                          type: 'GET',
                          url: '/login-user-validator?username='+user,
                          success: function(data){
                            if(data!=="false"){
                              if(data.slice(0,9)==="./uploads"){
