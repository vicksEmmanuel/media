          function allowDrop(event) {
                event.preventDefault();
            }
          function drag(event,element) {
                var x=element.parentNode.parentNode.nextElementSibling.nextElementSibling.files;
                data=Number(event.target.getAttribute('data-number'));
                event.dataTransfer.clearData();
                  for(var i=0;i<x.length; i++){
                    if(i!==data){
                      event.dataTransfer.items.add(x[i]);
                    }
                  }
                element.parentNode.parentNode.nextElementSibling.nextElementSibling.files=event.dataTransfer.files;
          }
          function drop(event,element) {
              event.preventDefault();
          }
          function ufo(event){
                event.preventDefault();
                if (event.dataTransfer.items) {
                  if(event.dataTransfer.items.length==1){
                    if (event.dataTransfer.items[0].kind === 'file') {
                      var file = event.dataTransfer.items[0].getAsFile();
                      if(/^video/.test(file.type) ){
                          var fileName=file.name;
                          var ext=fileName.slice((Math.max(0, fileName.lastIndexOf(".")) || Infinity) + 1);
                          var pos = fileName.lastIndexOf(".");
                          if (pos > 0) {
                              fileName = fileName.substring(0, pos);
                          }
                          fileName+="_"+Date.now();
                          fileName+="."+ext;
                          treeName=fileName;
                          treeName=treeName.trim();
                          treeName=treeName.replace(/ +?/g, '');
                          upload_video(file, treeName);
                          $(".upload-video-for-users").addClass("disab");
                          $("#upload-user-create-album").addClass("disab");
                          $(".upload-user-create-vide-clickto").html("#Wait");
                          $(".upload-user-create-vide-clickto-or").html("#for");
                          $(".upload-user-create-vide-upload").html("#Upload");
                          $(".upload-user-create-video-video").hide();
                      }
                    }
                  }
                }
                $("#upload-user-create-album").css({"border": "0px", "border-width": "thin"});
                removeDragData(event);
          }
          function removeDragData(ev) {
            if (ev.dataTransfer.items) {
              ev.dataTransfer.items.clear();
            } else {
              ev.dataTransfer.clearData();
            }
          }
          function ufover(event){
              event.preventDefault();
              $("#upload-user-create-album").css({"border": "4px solid red", "border-width": "thin"});
          }
          function ufout(event){
              event.preventDefault();
              $("#upload-user-create-album").css({"border": "0px", "border-width": "thin"});
          }
          function upload_video(file, name){
                $("#upload-progress-bar").show();
                var loaded = 0;
                var step = 1024*20//1048576//1024*1024;
                var total = file.size;
                var start = 0;
                var read=0;
                var totalFailures=0;
                var reader = new FileReader();
                reader.onload = function(e){
                    var d = {
                        file: reader.result,
                         file_name: ""+name+"",
                         number: read++,
                         file_type: file.type
                    };
                    $.ajax({
                        url:"/upload-chunk",
                        type:"POST",
                        cache: false,
                        data:d,
                        success: function(data){
                          totalFailures=0;
                          loaded += step;
                          var percent=Math.floor((loaded/total) * 100);
                          $("#upload-progress-bar").html(percent+"%");
                          $("#upload-progress-bar").css("width",`${percent}%`);
                              if(loaded <= total){
                                  blob = file.slice(loaded,loaded+step);
                                  reader.readAsBinaryString(blob);
                              } else {
                                  loaded = total;
                                  $("#upload-progress-bar").html(`90%`);
                                  $("#upload-progress-bar").css("width",`90%`);
                                  $(".upload-user-create-vide-upload").html("#Processing");
                                  $.ajax({
                                      async: true,
                                      url:`/upload-tell?data=${name}`,
                                      xhr: function () {
                                          var xhr = new window.XMLHttpRequest();
                                          xhr.upload.addEventListener("progress", function (evt) {
                                              if (evt.lengthComputable) {
                                                    var percentComplete = evt.loaded / evt.total;
                                                    $("#ruler").css("width", ""+percentComplete*100+"%");
                                              }
                                          }, false);
                                          xhr.addEventListener("progress", function (evt) {
                                              if (evt.lengthComputable) {
                                                  var percentComplete = evt.loaded / evt.total;
                                                  $("#ruler").css("width", ""+percentComplete*100+"%");
                                              }
                                          }, false);
                                          return xhr;
                                      },
                                      type:"GET",
                                      success: function(data){
                                        data=data.replace("./uploads","");
                                        var file_name=name.replace(".","_");
                                        data+=`/image/thumbnailArt.png`;
                                        if(! ($("#thumbnail-artwork").get(0).files &&   $("#thumbnail-artwork").get(0).files[0])){
                                          var img=new Image();
                                          img.src=data;
                                          img.onload=function(){
                                            var canvas = document.createElement("canvas");
                                            canvas.width =this.width;
                                            canvas.height =this.height;
                                            var ctx = canvas.getContext("2d");
                                            ctx.drawImage(this, 0, 0);
                                            var dataURL = canvas.toDataURL("image/png");
                                            $(".thumb-appear").css({"background":"url('"+dataURL+"') no-repeat center","background-size":"cover","width":"80%", "height": "70px"});
                                          }
                                        }
                                        $("#upload-progress-bar").html(percent+"%");
                                        $("#upload-progress-bar").css("width",`${percent}%`);
                                        $(".upload-user-create-vide-clickto").html("#Done");
                                        $(".upload-user-create-vide-clickto-or").hide();
                                        $(".upload-user-create-vide-upload").hide();
                                        $(".video-submit-block").show(200);
                                      }
                                    });
                              }
                        },
                        error: function(data){
                              if((totalFailure++) < 3) {
                                console.log("retry");
                               reader.readAsBinaryString(blob);
                              } else {
                                console.log("Error");
                              }
                        }
                      });
                  };
                var blob = file.slice(start,step);
                reader.readAsBinaryString(blob);
          }
