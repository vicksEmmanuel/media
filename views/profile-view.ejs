                loadNextPage = (function() {
                    var pageNumber = 0;
                    return function () {
                        return new Promise(function(resolve, reject) {
                            $.ajax({
                              async: true,
                              type:'GET',
                              url: '/all-link?user=<%=locals.data.id%>',
                              success:function(data){
                                  if(data.length>0){
                                    var items = [];
                                    for (var id = pageNumber*pageSize, lastId = id + pageSize - 1; id <= lastId; ++id) {
                                        if(id<data.length){
                                              $("#js-loading").show();
                                              if(data[id].type=="process"){
                                                items.push({
                                                  id: id,
                                                  type: data[id].type,
                                                  file_name: data[id].file_name
                                                });
                                              }
                                              else if(data[id].type=="audio"){
                                                items.push({
                                                    id: id,
                                                    artist_name:data[id].artist_name,
                                                    album_artwork_path : data[id].album_artwork_path,
                                                    album_name: data[id].album_name,
                                                    audio_name: data[id].audio_name,
                                                    audio_path: data[id].audio_path,
                                                    pcm: data[id].pcm,
                                                    date_created: data[id].date_created,
                                                    genres: data[id].genres,
                                                    description: data[id].description,
                                                    privacy: data[id].privacy,
                                                    type: data[id].type,
                                                    year_created: data[id].year_created,
                                                    user_id: data[id].user_id,
                                                    ide: data[id].id,
                                                    album_own_artwork: data[id].album_own_artwork
                                                })
                                              }else if(data[id].type=="video"){
                                                items.push({
                                                    id: id,
                                                    category: data[id].category,
                                                    date_created: data[id].date_created,
                                                    playlist_id: data[id].playlist_id,
                                                    thubnail_path: data[id].thubnail_path,
                                                    type: data[id].type,
                                                    video_description: data[id].video_description,
                                                    video_path: data[id].video_path,
                                                    video_privacy: data[id].video_privacy,
                                                    video_title: data[id].video_title,
                                                    user_id: data[id].user_id,
                                                    ide: data[id].id
                                                })
                                              }else if(data[id].type=="playlist"){
                                                items.push({
                                                  id: id,
                                                  ide: data[id].id,
                                                  playlist_name: data[id].playlist_name,
                                                  privacy: data[id].privacy,
                                                  type: data[id].type,
                                                  user_id: data[id].user_id,
                                                  date_created: data[id].date_created
                                                })
                                              }else if(data[id].type=="album"){
                                                items.push({
                                                  id: id,
                                                  album_artwork_path: data[id].album_artwork_path,
                                                  album_name: data[id].album_name,
                                                  album_path: data[id].album_path,
                                                  artist_name: data[id].artist_name,
                                                  description: data[id].description,
                                                  genres: data[id].trap,
                                                  ide: data[id].id,
                                                  privacy: data[id].privacy,
                                                  type: data[id].type,
                                                  user_id: data[id].user_id
                                                })
                                              }
                                        }else{
                                          $("#js-loading").hide();
                                        }
                                    }
                                    pageNumber++;
                                    setTimeout(function() { resolve(items); }, getRandomInRange(500, 4000));
                                  }else{
                                          $("#js-loading").hide();
                                  }
                                }
                            });
                        });
                    }
                })(),
                getRandomInRange = function(min, max) {
                    return Math.floor(Math.random() * (max - min + 1)) + min;
                },
                listView = document.querySelector('.listview'),
                sentinelObserver = new IntersectionObserver(sentinelListener, {threshold: 1}) ;
                nextPage().then(() => {
                    nextPage().then(updateSentinel);
                });
    </script>
